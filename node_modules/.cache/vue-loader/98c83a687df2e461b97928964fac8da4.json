r\n                <v-btn :href=\"source\" icon large target=\"_blank\" v-on=\"on\">\r\n                  <v-icon>mdi-code-tags</v-icon>\r\n                </v-btn>\r\n              </template>\r\n              <span>Source</span>\r\n            </v-tooltip>\r\n            <v-tooltip right>\r\n              <template v-slot:activator=\"{ on }\">\r\n                <v-btn\r\n                  icon\r\n                  large\r\n                  href=\"https://codepen.io/johnjleider/pen/pMvGQO\"\r\n                  target=\"_blank\"\r\n                  v-on=\"on\"\r\n                >\r\n                  <v-icon>mdi-codepen</v-icon>\r\n                </v-btn>\r\n              </template>\r\n              <span>Codepen</span>\r\n            </v-tooltip>\r\n          </v-toolbar>\r\n          <v-card-text>\r\n            <v-form>\r\n              <v-text-field\r\n                label=\"Login\"\r\n                name=\"login\"\r\n                prepend-icon=\"person\"\r\n                v-model.trim=\"loginForm.email\"\r\n                type=\"text\"\r\n                placeholder=\"you@email.com\"\r\n                id=\"email1\">\r\n              </v-text-field>\r\n\r\n              <v-text-field\r\n                label=\"Password\"\r\n                name=\"password\"\r\n                prepend-icon=\"lock\"\r\n                v-model.trim=\"loginForm.password\"\r\n                type=\"password\"\r\n                placeholder=\"******\"\r\n                id=\"password1\">\r\n              </v-text-field>\r\n            </v-form>\r\n          </v-card-text>\r\n          <v-card-actions>\r\n            <v-spacer></v-spacer>\r\n            <v-btn color=\"primary\" @click=\"login\">Login</v-btn>\r\n          </v-card-actions>\r\n        </v-card>\r\n      </v-flex>\r\n    </v-layout>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nconst fb = require(\"../firebase.config.js\");\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      loginForm: {\r\n        email: \"\",\r\n        password: \"\"\r\n      },\r\n      signupForm: {\r\n        name: \"\",\r\n        title: \"\",\r\n        email: \"\",\r\n        password: \"\"\r\n      },\r\n      showLoginForm: false\r\n    };\r\n  },\r\n  props: {\r\n    source: String\r\n  },\r\n  methods: {\r\n    toggleForm() {\r\n      this.showLoginForm = !this.showLoginForm;\r\n    },\r\n    login() {\r\n      fb.auth\r\n        .signInWithEmailAndPassword(\r\n          this.loginForm.email,\r\n          this.loginForm.password\r\n        )\r\n        .then(user => {\r\n          ///NOTE CHANGED FROM USER.USER TO USER\r\n          this.$store.commit(\"setCurrentUser\", user.user);\r\n          this.$store.dispatch(\"fetchUserProfile\");\r\n          this.$router.push(\"/dashboard\");\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    signup() {\r\n      fb.auth\r\n        .createUserWithEmailAndPassword(\r\n          this.signupForm.email,\r\n          this.signupForm.password\r\n        )\r\n        .then(user => {\r\n          ///NOTE CHANGED FROM USER.USER TO USER\r\n          this.$store.commit(\"setCurrentUser\", user.user);\r\n\r\n          // create user obj\r\n          fb.usersCollection\r\n            .doc(user.user.uid)\r\n            .set({\r\n              name: this.signupForm.name,\r\n              title: this.signupForm.title\r\n            })\r\n            .then(() => {\r\n              this.$store.dispatch(\"fetchUserProfile\");\r\n              this.$router.push(\"/dashboard\");\r\n            })\r\n            .catch(err => {\r\n              console.log(err);\r\n            });\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>"]}]}