A,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAbA;AAcA,KAnBA;AAoBA,IAAA,MApBA,oBAoBA;AAAA;;AACA,MAAA,EAAA,CAAA,IAAA,CACA,8BADA,CAEA,KAAA,UAAA,CAAA,KAFA,EAGA,KAAA,UAAA,CAAA,QAHA,EAKA,IALA,CAKA,UAAA,IAAA,EAAA;AACA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,gBAAA,EAAA,IAAA,CAAA,IAAA,EAFA,CAIA;;;AACA,QAAA,EAAA,CAAA,eAAA,CACA,GADA,CACA,IAAA,CAAA,IAAA,CAAA,GADA,EAEA,GAFA,CAEA;AACA,UAAA,IAAA,EAAA,MAAA,CAAA,UAAA,CAAA,IADA;AAEA,UAAA,KAAA,EAAA,MAAA,CAAA,UAAA,CAAA;AAFA,SAFA,EAMA,IANA,CAMA,YAAA;AACA,UAAA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,kBAAA;;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,YAAA;AACA,SATA,EAUA,KAVA,CAUA,UAAA,GAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,SAZA;AAaA,OAvBA,EAwBA,KAxBA,CAwBA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OA1BA;AA2BA;AAhDA;AAhBA,CAAA","sourcesContent":["<template>\r\n  <div id=\"login\">\r\n    <section>\r\n    <div class=\"col2\" :class=\"{ 'signup-form': !showLoginForm }\">\r\n        <form v-if=\"showLoginForm\" @submit.prevent>\r\n          <h1>Welcome Back</h1>\r\n\r\n          <label for=\"email1\">Email</label>\r\n          <input v-model.trim=\"loginForm.email\" type=\"text\" placeholder=\"you@email.com\" id=\"email1\" />\r\n\r\n          <label for=\"password1\">Password</label>\r\n          <input\r\n            v-model.trim=\"loginForm.password\"\r\n            type=\"password\"\r\n            placeholder=\"******\"\r\n            id=\"password1\"\r\n          />\r\n\r\n          <button @click=\"login\" class=\"button\">Log In</button>\r\n\r\n          <div class=\"extras\">\r\n            <a>Forgot Password</a>\r\n            <a  @click=\"toggleForm\">Create an Account</a>\r\n          </div>\r\n        </form>\r\n\r\n        <form v-else @submit.prevent>\r\n          <h1>Get Started</h1>\r\n\r\n          <label for=\"name\">Name</label>\r\n          <input v-model.trim=\"signupForm.name\" type=\"text\" placeholder=\"Savvy Apps\" id=\"name\" />\r\n\r\n          <label for=\"title\">Title</label>\r\n          <input v-model.trim=\"signupForm.title\" type=\"text\" placeholder=\"Company\" id=\"title\" />\r\n\r\n          <label for=\"email2\">Email</label>\r\n          <input\r\n            v-model.trim=\"signupForm.email\"\r\n            type=\"text\"\r\n            placeholder=\"you@email.com\"\r\n            id=\"email2\"\r\n          />\r\n\r\n          <label for=\"password2\">Password</label>\r\n          <input\r\n            v-model.trim=\"signupForm.password\"\r\n            type=\"password\"\r\n            placeholder=\"min 6 characters\"\r\n            id=\"password2\"\r\n          />\r\n\r\n          <button @click=\"signup\" class=\"button\">Sign Up</button>\r\n\r\n          <div class=\"extras\">\r\n            <a @click=\"toggleForm\">Back to Log In</a>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nconst fb = require(\"../firebase.config.js\");\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      loginForm: {\r\n        email: \"\",\r\n        password: \"\"\r\n      },\r\n      signupForm: {\r\n        name: \"\",\r\n        title: \"\",\r\n        email: \"\",\r\n        password: \"\"\r\n      },\r\n      showLoginForm: false\r\n    };\r\n  },\r\n  methods: {\r\n    toggleForm() {\r\n      this.showLoginForm = !this.showLoginForm;\r\n    },\r\n    login() {\r\n      fb.auth\r\n        .signInWithEmailAndPassword(\r\n          this.loginForm.email,\r\n          this.loginForm.password\r\n        )\r\n        .then(user => {\r\n           ///NOTE CHANGED FROM USER.USER TO USER\r\n          this.$store.commit('setCurrentUser', user.user) \r\n          this.$store.dispatch(\"fetchUserProfile\");\r\n          this.$router.push(\"/dashboard\");\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    signup() {\r\n      fb.auth\r\n        .createUserWithEmailAndPassword(\r\n          this.signupForm.email,\r\n          this.signupForm.password\r\n        )\r\n        .then(user => {\r\n          ///NOTE CHANGED FROM USER.USER TO USER\r\n          this.$store.commit('setCurrentUser', user.user) \r\n\r\n          // create user obj\r\n          fb.usersCollection\r\n            .doc(user.user.uid)\r\n            .set({\r\n              name: this.signupForm.name,\r\n              title: this.signupForm.title\r\n            })\r\n            .then(() => {\r\n              this.$store.dispatch(\"fetchUserProfile\");\r\n              this.$router.push(\"/dashboard\");\r\n            })\r\n            .catch(err => {\r\n              console.log(err);\r\n            });\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/views"}]}